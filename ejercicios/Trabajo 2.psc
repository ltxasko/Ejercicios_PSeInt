Algoritmo Trabajo2
	Definir primer Como Entero
	Definir ssegundo Como Entero
	
	Escribir "Escriba el primer número.";
	Leer primer;
	
	Escribir "Escriba el ssegundo número.";
	Leer ssegundo;
	
	si(primer < ssegundo) Entonces
		Escribir primer + ssegundo;
	SiNo
		si(primer > ssegundo) Entonces
			Escribir primer - ssegundo;
		
FinSi
FinSi
FinAlgoritmo
